@model List<Vcard>



<div class="page-wrapper">
    <div class="content container-fluid">

        <div class="page-header">
            <div class="content-page-header">
                <h5>Datatable</h5>
            </div>
        </div>

        <div class="row">
            <div class="col-sm-12">
                <div class="card">
                    <div class="card-header card-buttons">
                        <h4 class="card-title">VCard List</h4>

                    </div>
                    <div class="card-body">
                        <div class="table-responsive">
                            <table class="datatable table table-stripped">
                                <thead>
                                    <tr>
                                        <th>First Name</th>
                                        <th>Last Name</th>
                                        <th>Mobile</th>
                                        <th>Phone</th>
                                        <th>Email</th>
                                        <th>Company</th>
                                        <th>Job Title</th>
                                        <th>Country</th>
                                        <th>State</th>
                                        <th>City</th>
                                        <th>Street</th>
                                        <th>Zip</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@Html.DisplayFor(modelItem => item.FirstName)</td>
                                            <td>@Html.DisplayFor(modelItem => item.LastName)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Mobile)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Phone)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Email)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Company)</td>
                                            <td>@Html.DisplayFor(modelItem => item.JobTitle)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Country)</td>
                                            <td>@Html.DisplayFor(modelItem => item.State)</td>
                                            <td>@Html.DisplayFor(modelItem => item.City)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Street)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Zip)</td>
                                        </tr>
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



